@model AsesoriasUABC.Models.Grupos

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section SubMenueOptions{
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
        </li>
        <li class="nav-item d-done d-sm-inline-block">
            <a href="@Url.Action("Index","Grupos")" class="nav-link"><i class="fas fa-arrow-alt-circle-left"></i> Regresar a la tabla </a>
        </li>
    </ul>
}
<h2>Borrar grupo</h2>
<div class="card">
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.num_grupo)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.num_grupo)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.carreras.nombre)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.carreras.nombre)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" />
            </div>
        }
    </div>
</div>


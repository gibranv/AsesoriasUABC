@model IEnumerable<AsesoriasUABC.Models.temasTb>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.numero_tema)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre_tema)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.numero_tema)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre_tema)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_Temas }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_Temas }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_Temas })
        </td>
    </tr>
}

</table>
